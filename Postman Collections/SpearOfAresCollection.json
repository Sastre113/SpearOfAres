									"listen": "prerequest",
									"script": {
										"id": "14268eda-dfbf-48b3-be62-6eda8aa6b4fa",
										"exec": [
											"const cryptoJs = require(\"crypto-js\");\r",
											"\r",
											"// Definir la función de encriptación\r",
											"function encryptMessage(message, key) {\r",
											"    let iv = new Uint8Array([\r",
											"        0x01, 0x00, 0x00, 0x00,\r",
											"        0x00, 0x00, 0x00, 0x00,\r",
											"        0x00, 0x00, 0x00, 0x00,\r",
											"        0x00, 0x00, 0x00, 0x00\r",
											"    ]);\r",
											"\r",
											"    iv = CryptoJS.lib.WordArray.create([0x01, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00]);\r",
											"    console.log(\"iv : \" + iv);\r",
											"\r",
											"    // Definir las opciones de encriptación\r",
											"    const options = {\r",
											"        iv: iv,\r",
											"        mode: cryptoJs.mode.CTR,\r",
											"        padding: cryptoJs.pad.NoPadding\r",
											"    };\r",
											"\r",
											"    // Encriptar el mensaje utilizando la clave y las opciones\r",
											"    const encrypted = cryptoJs.AES.encrypt(message, key, options);\r",
											"\r",
											"    // Obtener el texto cifrado en formato Base64\r",
											"    const ciphertext = encrypted.ciphertext.toString(cryptoJs.enc.Base64);\r",
											"\r",
											"    // Obtener el valor del vector de inicialización (IV) en formato Base64\r",
											"    const ivText = encrypted.iv.toString(cryptoJs.enc.Base64);\r",
											"\r",
											"    console.log(\"ciphertext base64 : \" + ciphertext);\r",
											"    console.log(\"ivtext base64 : \" + ivText);\r",
											"    // Devolver el texto cifrado y el IV concatenados con un separador\r",
											"    return ivText + ciphertext;\r",
											"}\r",
											"\r",
											"// Ejemplo de uso\r",
											"const mensaje = \"Hola, este es un mensaje secreto.\";\r",
											"const clave = \"NoMeRobesPorFavo\";\r",
											"\r",
											"const mensajeEncriptado = encryptMessage(mensaje, clave);\r",
											"console.log(\"Mensaje encriptado:\", mensajeEncriptado);\r",
											"pm.collectionVariables.set(\"hashGenerado\", mensajeEncriptado);"
										],
										"type": "text/javascript"
									}
								}
